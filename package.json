{
  "name": "esafety-threat-detection",
  "version": "1.0.0",
  "description": "AI-powered content safety analysis platform",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "test": "jest",
    "deploy": "npm run build && node deploy.js"
  },
  "keywords": [
    "safety",
    "ai",
    "content-moderation",
    "threat-detection"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "axios": "^1.6.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "openai": "^4.24.1",
    "google-auth-library": "^9.4.1",
    "@google/generative-ai": "^0.1.3",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ytdl-core": "^4.11.5",
    "link-preview-js": "^3.0.5",
    "natural": "^6.10.2",
    "compromise": "^14.10.0",
    "@distube/ytdl-core": "^4.13.3",
    "youtube-transcript": "^1.2.1",
    "node-fetch": "^2.7.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
